<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.colegiosaltamontes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/splash_screen_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/splash_screen_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ColegioSaltaMontes">
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EstadisticasActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".RegistroActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".AyudaActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".DetalleActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>